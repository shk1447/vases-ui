          "react-hooks/exhaustive-deps": "off",
          "react/jsx-pascal-case": "off",
          "@typescript-eslint/no-useless-constructor": "off"
        }
      }
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
